{
    "version": 1,
    "builds": [
      {
        "src": "frontend/dist/**",
        "use": "@vercel/static"
      },
      {
        "src": "backend/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "backend/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/dist/$1"
      }
    ]
  }